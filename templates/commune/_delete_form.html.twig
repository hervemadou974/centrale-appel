<form method="post" action="{{ path('app_commune_delete', {'id': commune.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette commune ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ commune.id) }}">
    <button class="btn btn-primary d-flex justify-content-center align-items-center" style="width: 300px;">Supprimer</button>
</form>
